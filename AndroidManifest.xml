<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.macbury.kontestplayer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="AppDelegate">
        <activity
            android:name="com.macbury.kontestplayer.AuditionsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".episodes.EpisodesActivity"
            android:parentActivityName="com.macbury.kontestplayer.episodes.EpisodesActivity">
            <meta-data
              android:name="android.support.PARENT_ACTIVITY"
              android:value="com.macbury.kontestplayer.episodes.EpisodesActivity" />
        </activity>
    </application>

</manifest>
